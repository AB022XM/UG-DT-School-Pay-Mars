<databaseChangeLog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity Student.
    -->
    <changeSet id="20230330170315-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="student_class_id"
                                 baseTableName="student"
                                 constraintName="fk_student__student_class_id"
                                 referencedColumnNames="id"
                                 referencedTableName="student_class"/>

        <addForeignKeyConstraint baseColumnNames="school_id"
                                 baseTableName="student"
                                 constraintName="fk_student__school_id"
                                 referencedColumnNames="id"
                                 referencedTableName="school"/>
    </changeSet>
</databaseChangeLog>
